{
  "name": "create-two-scripts",
  "version": "1.0.7-alpha.0",
  "description": "一个简易的前端脚手架工具",
  "keywords": [
    "react",
    "typescript",
    "babel",
    "sass",
    "scss",
    "webpack v5"
  ],
  "bin": {
    "two": "bin/index.js"
  },
  "main": "index.js",
  "scripts": {
    "publish": "npm publish",
    "unpublish": "npm unpublish --force create-two-scripts",
    "release": "standard-version -r patch",
    "eslint:test": "eslint './**/*' --ext .js,.ts",
    "lint:fix": "eslint './**/*' --fix --ext .js,.ts",
    "prettier": "prettier './**/*' --write "
  },
  "author": "wulin",
  "license": "MIT",
  "engines": {
    "node": ">=10.13.0"
  },
  "bugs": {
    "url": "https://github.com/qiuysh/create-two-scripts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qiuysh/create-two-scripts"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "lib/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "config/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "util/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.17.2",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.3",
    "chalk": "^4.1.0",
    "commander": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "esbuild-loader": "^2.18.0",
    "fs-extra": "^10.0.0",
    "handlebars": "^4.7.7",
    "html-webpack-plugin": "^5.5.0",
    "inquirer": "^7.3.3",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.5.3",
    "ora": "^5.3.0",
    "postcss": "^8.4.8",
    "postcss-loader": "^6.2.1",
    "react-refresh": "^0.11.0",
    "sass": "^1.49.8",
    "sass-loader": "^12.6.0",
    "shelljs": "^0.8.4",
    "style-loader": "^3.3.1",
    "thread-loader": "^3.0.4",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "webpack": "^5.68.0",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^6.6.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.28.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "standard-version": "^9.1.0"
  }
}
